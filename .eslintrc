{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": [2, {"vars": "all", "argsIgnorePattern": "^_"}],
    "no-console": 0
  },
  "babelOptions": {
    "configFile": "./.babel-eslintrc"
 }
}